{
  "executionRoleArn": "arn:aws:iam::558627362683:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 5000,
          "protocol": "tcp",
          "containerPort": 5000
        }
      ],
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "name": "uniandes-blacklist-container",
      "environment": [
        {
          "name": "NEW_RELIC_APP_NAME",
          "value": "blacklist-service"
        },
        {
          "name": "NEW_RELIC_CONFIG_FILE",
          "value": "/app/newrelic.ini"
        },
        {
          "name": "RUN_DB_MIGRATIONS",
          "value": "1"
        }
      ],
      "secrets": [
        {
          "name": "NEW_RELIC_LICENSE_KEY",
          "valueFrom": "arn:aws:ssm:us-east-2:558627362683:parameter/newrelic-license-key"
        },
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:ssm:us-east-2:558627362683:parameter/blacklist-service/DATABASE_URL"
        },
        {
          "name": "AUTH_BEARER_TOKEN",
          "valueFrom": "arn:aws:ssm:us-east-2:558627362683:parameter/blacklist-service/AUTH_BEARER_TOKEN"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "512",
  "memory": "1024",
  "family": "uniandes-blacklist-task"
}
